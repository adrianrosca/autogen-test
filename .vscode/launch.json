{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Generate",
			"skipFiles": ["<node_internals>/**"],
			"runtimeExecutable": "pnpm",
			"runtimeArgs": ["run", "generate"],
			"outFiles": ["${workspaceFolder}/src/_generated/**/*.js"],
			"preLaunchTask": "tsc: build - tsconfig.json",
			"sourceMaps": true,
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
